{
    "name": "ent-mut-bo",
    "description": "ent-mut-bo",
    "keywords": [
        "UniversitÃ© Paris-Saclay"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "bin-dir": "vendor/bin/",
        "discard-changes": true
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ent/cssmin",
                "version": "3.0.1",
                "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
                "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ent/jsminplus",
                "version": "1.4",
                "dist": { "url": "http://tweakimg.net/files/upload/jsminplus-1.4.zip", "type": "zip" },
                "autoload": { "classmap": [ "jsminplus.php" ] }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ent/minifycsscompressor",
                "version": "1.0",
                "dist": { "url": "http://minify.googlecode.com/svn/trunk/min/lib/Minify/CSS/Compressor.php", "type": "file" },
                "autoload": { "classmap": [ "Compressor.php" ] }
            }
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.2.4",
        "doctrine/doctrine-module": "~0.5",
        "doctrine/doctrine-orm-module": "~0.4",
        "doctrine/orm": "~2.3.0",
        "kriswallsmith/assetic": "~1.2",
        "RWOverdijk/AssetManager": "~1.4",
        "PHPOffice/PHPWord": "~0.12",
        "league/csv": "~6.0",
        "qu-modules/qu-tcpdf":"dev-master",
        "ent/cssmin": "*",
        "ent/jsminplus": "*",
        "ent/minifycsscompressor":"*"
    },
    "require-dev": {
        "zendframework/zftool": "~0.1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "rm -Rf data/cache",
            "mkdir -p data/cache",
            "mkdir -p data/logs/ent_sql",
            "mkdir -p data/logs/ent_errors",
            "mkdir -p data/logs/ent_authent",
            "mkdir -p data/logs/ent_actions",
            "chmod -R 777 data"
        ]
    }
}
